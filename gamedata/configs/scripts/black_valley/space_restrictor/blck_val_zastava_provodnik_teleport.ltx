[logic]
active = sr_idle@scene_teleport_to_hutor

[sr_idle@scene_teleport_to_hutor]
on_info = {+blck_val_dialog_actor_go_to_hutor} sr_idle@teleport_actor %=RavenMemmoryStart =run_postprocess(black:2525:true) +blck_val_provodnik_teleport_to_hutor_again -blck_val_dialog_actor_go_to_zavod -blck_val_provodnik_teleport_to_zavod_again%

[sr_idle@teleport_actor]
on_timer = 2000 | sr_idle@stop_postprocess %=teleport_actor(actor_from_provodnik_in_hutor_walk:actor_from_provodnik_in_hutor_look)%

[sr_idle@stop_postprocess]
on_timer = 2000 | sr_idle@enable_ui %=stop_postprocess(2525)%

[sr_idle@enable_ui]
on_timer = 100 | sr_idle@give_info %=enable_ui%

[sr_idle@give_info]
on_timer = 100 | sr_idle@scene_teleport_to_zavod

[sr_idle@scene_teleport_to_zavod]
on_info = {+blck_val_dialog_actor_go_to_zavod} sr_idle@teleport_actor_zavod %=RavenMemmoryStart =run_postprocess(black:2525:true) +blck_val_provodnik_teleport_to_zavod_again -blck_val_dialog_actor_go_to_hutor -blck_val_provodnik_teleport_to_hutor_again%

[sr_idle@teleport_actor_zavod]
on_timer = 2000 | sr_idle@stop_postprocess_zavod %=teleport_actor(actor_from_provodnik_in_zavod_walk:actor_from_provodnik_in_zavod_look)%

[sr_idle@stop_postprocess_zavod]
on_timer = 2000 | sr_idle@enable_ui_zavod %=stop_postprocess(2525)%

[sr_idle@enable_ui_zavod]
on_timer = 100 | sr_idle@give_info_zavod %=enable_ui%

[sr_idle@give_info_zavod]
on_timer = 100 | sr_idle@scene_teleport_to_hutor

[sr_idle@nil]