;=========meet_quest_line========
[meet@quest_line]
close_anim 		= nil
close_victim 	= nil
far_anim 		= nil
far_victim 		= nil
close_distance 	= 0
far_distance 	= 0
use = {=actor_enemy} false, {=dist_to_actor_le(3)} true, false
snd_on_use = {!dist_to_actor_le(3)} nil
meet_on_talking = false
;==================================

[logic]
active = walker@blck_val_actor_go_to_quest
level_spot = quest_npc
post_combat_time = 0, 0
suitable = {=check_npc_name(blck_val_final_scene_actor)} true, false
prior = 200

[walker@blck_val_actor_go_to_quest]
path_walk = blck_val_final_scene_actor_walk_1
def_state_moving = walk_noweap
meet = no_meet
on_info = {+blck_val_actor_go_to_quest_complete} walker@blck_val_final_scene_actor
combat_ignore_cond = true 
combat_ignore_keep_when_attacked = true
gather_items_enabled = false
help_wounded_enabled = false
corpse_detection_enabled = false
invulnerable = true
on_signal = blck_val_final_scene_actor_come | {-blck_val_actor_go_to_quest_complete} walker@blck_val_final_scene_actor %+blck_val_actor_go_to_quest_complete%

[walker@blck_val_final_scene_actor]
path_walk = blck_val_final_scene_actor_walk
def_state_moving = walk_noweap
meet = no_meet
combat_ignore_cond = true
combat_ignore_keep_when_attacked = true
gather_items_enabled = false
help_wounded_enabled = false
corpse_detection_enabled = false
invulnerable = true