[meet@start_quest]:meet@quest_line
close_victim = actor
allow_break = false
meet_dialog = blck_val_actor_meet_from_provodnik
trade_enable = false

[meet@in_hutor]:meet@quest_line
close_victim = actor
use = {!actor_enemy +blck_val_two_dialog_provodnik_give -blck_val_two_dialog_provodnik_complete} self, true
allow_break = false
meet_dialog = blck_val_actor_meet_from_provodnik
trade_enable = false

[meet@in_zavod]:meet@quest_line
close_victim = actor
use = {!actor_enemy +blck_val_dialog_from_zavod_teleport_give -blck_val_dialog_from_zavod_teleport_complete} self, true
allow_break = false
meet_dialog = blck_val_actor_meet_from_provodnik
trade_enable = false