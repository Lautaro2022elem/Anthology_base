[logic]
active = sr_idle@wait1

[sr_idle@wait1]
on_info = {+flag_run_lift_1a -flag_run_lift_1b -flag_run_lift_1c} sr_idle@wait2 %-flag_run_lift_1a +flag_run_lift_1b =stop_tutorial =anim_obj_forward(station_digger_lift_door_1_1)% 
on_info2 = {+flag_run_lift_1c} sr_idle@run2 

[sr_idle@wait2]
on_info = {+flag_run_lift_1c} sr_idle@run1 
on_game_timer = 4 | {+flag_run_lift_1b -flag_run_lift_1c} sr_idle@start %=anim_obj_forward(station_digger_lift_door_1_2)%

[sr_idle@start]
on_info = {+flag_run_lift_1c} sr_idle@run1 %+flag_run_lift_1b%
on_game_timer = 145 | {+flag_run_lift_1b -flag_run_lift_1s1 -flag_run_lift_1c} %+flag_run_lift_1s1 =anim_obj_backward(station_digger_lift_door_1_1)% 
on_game_timer2 = 150 | {+flag_run_lift_1b -flag_run_lift_1s2 -flag_run_lift_1c} %+flag_run_lift_1s2 =anim_obj_backward(station_digger_lift_door_1_2)% 
on_game_timer3 = 185 | {+flag_run_lift_1b +flag_run_lift_1s1 +flag_run_lift_1s2 -flag_run_lift_1c} sr_idle@wait1 %-flag_run_lift_1b -flag_run_lift_1s1 -flag_run_lift_1s2 =anim_obj_stop(station_digger_lift_door_1_1) =anim_obj_stop(station_digger_lift_door_1_2)% 

[sr_idle@run1]
on_info = {+flag_run_lift_1b +flag_run_lift_1c -flag_run_lift_1s1} %+flag_run_lift_1s1 =anim_obj_backward(station_digger_lift_door_1_1)%
on_game_timer = 4 | {+flag_run_lift_1b +flag_run_lift_1c -flag_run_lift_1s2} %+flag_run_lift_1s2 =anim_obj_backward(station_digger_lift_door_1_2)%
on_game_timer2 = 35 | {+flag_run_lift_1b +flag_run_lift_1s1 +flag_run_lift_1s2 +flag_run_lift_1c} %-flag_run_lift_1c -flag_run_lift_1b -flag_run_lift_1s1 -flag_run_lift_1s2  =anim_obj_stop(station_digger_lift_door_1_1) =anim_obj_stop(station_digger_lift_door_1_2)% 
on_game_timer3 = 55 | {-flag_run_lift_1b -flag_run_lift_1c} sr_idle@run3 %=eff_lift_run()%

[sr_idle@run2]
on_game_timer = 5 | {+flag_run_lift_1c} sr_idle@run3 %-flag_run_lift_1c =eff_lift_run()%

[sr_idle@run3]
on_game_timer = 80 | sr_idle@wait1 %=eff_lift_stop() =station_digger_lift_run +flag_run_lift_2a%