            alife():release(alife():object(v),true)
         end
      end      		  
      SItemInInv("bar_a2_spec_juk_item_2")
	  SItemInInv("bar_a2_spec_juk_item_2")
	  SItemInInv("bar_a2_spec_juk_item_2")
	  SItemInInv("bar_a2_spec_juk_item_2")
	  SItemInInv("bar_a2_spec_juk_item_2")
