function no_more_reward(first_speaker, second_speaker)
	local fight_type = {"assalut","duel","hunt","wantlive"}
	resault = true
	for i = 1, 5 do
		for j, k in ipairs(fight_type) do
			if has_alife_info("bar_arena_enemy_"..k.."_"..tostring(i).."_death") 
					and not has_alife_info("take_reward_"..k.."_"..tostring(i)) 
				then resault = false 
			end
		end
 	end
	for i = 1, 5 do
		k = "bodyguard"
		if has_alife_info("bar_arena_enemy_"..k.."_"..tostring(i).."_death") 
				and not has_alife_info("take_reward_"..k.."_"..tostring(i))
				and not has_alife_info("arena_"..k.."_"..tostring(i).."_fall")
			then resault = false 
		end
	end
	for i = 1, 5 do
		k = "friend"
		if has_alife_info("bar_arena_enemy_"..k.."_"..tostring(i).."_death") 
				and not has_alife_info("take_reward_"..k.."_"..tostring(i))
				and not has_alife_info("arena_"..k.."_"..tostring(i).."_fall")
			then resault = false 
		end
	end
return resault
end


