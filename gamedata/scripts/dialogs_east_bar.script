----------------------Очистка арены и инвентаря ГГ-------------------------------------------------
function clear_arena_after_fight(first_speaker, second_speaker)
	if xr_conditions.squad_exist(db.actor,nil,{"bodyguard_1_squad"}) then
		xr_effects.remove_squad(db.actor,nil,{"bodyguard_1_squad"})
		xr_effects.create_squad(db.actor,nil,{"bodyguard_1_squad_base","bar_smart_friend_base"})
	end
	if xr_conditions.squad_exist(db.actor,nil,{"bodyguard_2_squad"}) then
		xr_effects.remove_squad(db.actor,nil,{"bodyguard_2_squad"})
		xr_effects.create_squad(db.actor,nil,{"bodyguard_2_squad_base","bar_smart_friend_base"})
	end
	if xr_conditions.squad_exist(db.actor,nil,{"bodyguard_3_squad"}) then
		xr_effects.remove_squad(db.actor,nil,{"bodyguard_3_squad"})
		xr_effects.create_squad(db.actor,nil,{"bodyguard_3_squad_base","bar_smart_friend_base"})
	end
	if xr_conditions.squad_exist(db.actor,nil,{"friend_1_squad"}) then
		xr_effects.remove_squad(db.actor,nil,{"friend_1_squad"})
		xr_effects.create_squad(db.actor,nil,{"friend_1_squad_base","bar_smart_friend_base"})
	end
	if xr_conditions.squad_exist(db.actor,nil,{"friend_2_squad"}) then
		xr_effects.remove_squad(db.actor,nil,{"friend_2_squad"})
		xr_effects.create_squad(db.actor,nil,{"friend_2_squad_base","bar_smart_friend_base"})
	end
	if xr_conditions.squad_exist(db.actor,nil,{"friend_3_squad"}) then
		xr_effects.remove_squad(db.actor,nil,{"friend_3_squad"})
		xr_effects.create_squad(db.actor,nil,{"friend_3_squad_base","bar_smart_friend_base"})
	end
	xr_effects.release_actor_inventory(db.actor,nil)
	xr_effects.clear_arena(db.actor,nil,{"level_bar_space_restrictor"})
	xr_effects.clear_arena(db.actor,nil,{"bar_arena_lestn_sr"})
end

