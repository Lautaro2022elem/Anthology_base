function bar_alf_actor_relocate_info_all(first_speaker, second_speaker)
   local items_table = {"labx5_quest_document_1","labx5_quest_document_2","labx5_quest_document_3"}
   for k,v in pairs(items_table) do
      if db.actor:object(v) ~= nil then
         dialogs.relocate_item_section_from_actor(first_speaker, second_speaker, v)			
      end
   end	
end

